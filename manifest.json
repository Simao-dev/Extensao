{
  "manifest_version": 3,
  "name": "Impressão de Seleção",
  "version": "1.0",
  "description": "Seleciona uma área da tela e a imprime.",
  "permissions": [
    "activeTab", 
    "scripting" 
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": "icon.png",
    "default_title": "Imprimir Seleção"
  }
}